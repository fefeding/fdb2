{
  "name": "database-tool",
  "version": "1.0.0",
  "description": "数据库管理工具",
  "private": true,
  "scripts": {
    "dev:midway": "cross-env NODE_ENV=local midway-bin dev --ts",
    "dev:vue": "cd web && npm run dev",
    "dev:all": "npm-run-all -p dev:vue dev:midway",
    "dev": "sh ./bin/dev.sh",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build:vue": "cd web && npm run build",
    "build:midway": "midway-bin build -c",
    "build": "sh ./bin/build.sh",
    "start": "pm2 start bootstrap.js --name database-tool --disable-logs",
    "restart": "pm2 restart database-tool --disable-logs",
    "stop": "pm2 stop database-tool"
  },
  "dependencies": {
    "@cicctencent/midwayjs-base": "^1.0.35",
    "@fefeding/base-server": "^0.0.37",
    "@fefeding/common": "^1.0.58",
    "@midwayjs/axios": "^3.20.11",
    "@midwayjs/bootstrap": "^3.20.11",
    "@midwayjs/busboy": "^3.20.16",
    "@midwayjs/cache-manager": "^3.20.11",
    "@midwayjs/cli": "^2.1.1",
    "@midwayjs/code-dye": "^3.20.16",
    "@midwayjs/core": "^3.20.11",
    "@midwayjs/decorator": "^3.20.11",
    "@midwayjs/info": "^3.20.16",
    "@midwayjs/koa": "^3.20.16",
    "@midwayjs/logger": "^4.0.1",
    "@midwayjs/static-file": "^3.20.16",
    "@midwayjs/swagger": "^3.20.17",
    "@midwayjs/typeorm": "^3.20.11",
    "@midwayjs/validate": "^3.20.17",
    "@midwayjs/view-nunjucks": "^3.20.17",
    "axios": "^1.13.2",
    "blueimp-md5": "^2.19.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "ip": "^2.0.1",
    "jm-jsruntime": "^1.0.7",
    "lodash": "^4.17.21",
    "mysql2": "^3.15.3",
    "oracledb": "^6.3.0",
    "reflect-metadata": "^0.2.2",
    "sqlserver": "latest",
    "swagger-ui-dist": "^5.30.3",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cicctencent/tars-deploy": "^1.0.8",
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/koa": "^3.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "mwts": "^1.3.0",
    "mwtsc": "^1.15.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "fefeding",
  "license": "MIT"
}